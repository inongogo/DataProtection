{
  "buildOptions": {
    "additionalArguments": [
      "/additionalfile:../../build/stylecop.json",
      "/ruleset:../../build/Analyzers.ruleset"
    ],
    "define": [
      "TRACE",
      "CODE_ANALYSIS"
    ],
    "preserveCompilationContext": true,
    "xmlDoc": true
  },
  "dependencies": {
    "Microsoft.AspNetCore.DataProtection": "1.0.0-rc2-*",
    "Microsoft.CodeAnalysis.FxCopAnalyzers": {
      "type": "build",
      "version": "1.1.0"
    },
    "Microsoft.Extensions.DependencyInjection.Abstractions": "1.0.0-rc2-*",
    "Microsoft.Extensions.Logging": "1.0.0-rc2-*",
    "StackExchange.Redis":  "1.1.603",
    "StyleCop.Analyzers": {
      "type": "build",
      "version": "1.0.0"
    },
    "System.Runtime.Analyzers": {
      "type": "build",
      "version": "1.1.0"
    }
  },
  "description": "ASP.NET Core extensions and utilities for working with DataProtection in a farm.",
  "frameworks": {
    "net46": {}
  },
  "packOptions": {
    "owners": [
      "Travis Illig"
    ]
  },
  "version": "0.0.1-*"
